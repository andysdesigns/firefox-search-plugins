<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
  <ShortName>Zing MP3</ShortName>
  <Description>Search songs on Zing MP3</Description>
  <InputEncoding>UTF-8</InputEncoding>
  <Image height="16" width="16">data:image/x-icon;base64,</Image>
  <UpdateInterval>1</UpdateInterval>
  <IconUpdateUrl>http://mp3.zing.vn/favicon.ico</IconUpdateUrl>
  <Url method="GET" resultDomain="mp3.zing.vn" template="http://mp3.zing.vn/tim-kiem/bai-hat.html" type="text/html">
    <Param name="q" value="{searchTerms}"/>
  </Url>
  <SearchForm>http://mp3.zing.vn/</SearchForm>
</SearchPlugin>
