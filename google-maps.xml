<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
  <ShortName>Google Maps</ShortName>
  <Description>Google Maps</Description>
  <InputEncoding>UTF-8</InputEncoding>
  <UpdateInterval>7</UpdateInterval>
  <IconUpdateUrl>https://maps.gstatic.com/favicon4.ico</IconUpdateUrl>
  <Image height="16" type="image/x-icon" width="16">https://maps.gstatic.com/favicon4.ico</Image>
  <Url method="GET" resultDomain="maps.google.com" template="https://maps.google.com/maps" type="text/html">
    <Param name="q" value="{searchTerms}"/>
    <Param name="hl" value="vi"/>
  </Url>
  <SearchForm>https://maps.google.com/</SearchForm>
</SearchPlugin>
