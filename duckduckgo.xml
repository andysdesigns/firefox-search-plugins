<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
  <ShortName>DuckDuckGo</ShortName>
  <Description>DuckDuckGo Search</Description>
  <InputEncoding>UTF-8</InputEncoding>
  <UpdateInterval>7</UpdateInterval>
  <IconUpdateUrl>https://duckduckgo.com/favicon.ico</IconUpdateUrl>
  <Image height="16" type="image/x-icon" width="16">https://duckduckgo.com/favicon.ico</Image>
  <Url method="POST" template="https://startpage.com/do/suggest" type="application/x-suggestions+json">
    <Param name="lang" value="english"/>
    <Param name="output" value="json"/>
    <Param name="query" value="{searchTerms}"/>
  </Url>
  <Url method="POST" template="https://duckduckgo.com/" type="text/html">
    <Param name="kg" value="p"/>
    <Param name="kw" value="w"/>
    <Param name="ko" value="s"/>
    <Param name="ke" value="-1"/>
    <Param name="k1" value="-1"/>
    <Param name="km" value="l"/>
    <Param name="k4" value="-1"/>
    <Param name="q" value="{searchTerms}"/>
  </Url>
  <SearchForm>https://duckduckgo.com/</SearchForm>
</SearchPlugin>
